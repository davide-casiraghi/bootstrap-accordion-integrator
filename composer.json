{
    "name": "davidecasiraghi/bootsrap-accordion-integrator",
    "description": "Add accordions in your application. The accordions are based on the Bootstrap 4 collapse component.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Casiraghi",
            "email": "davide.casiraghi@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "DavideCasiraghi\\BootstrapAccordion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavideCasiraghi\\BootstrapAccordion\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "~3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Barryvdh\\Debugbar\\ServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Barryvdh\\Debugbar\\Facade"
            }
        }
    }
}
